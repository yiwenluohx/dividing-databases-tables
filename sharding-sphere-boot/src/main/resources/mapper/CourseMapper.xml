<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.study.shardingsphereboot.dao.CourseMapper">
    <insert id="addCourse" parameterType="com.study.shardingsphereboot.model.Course">
        insert into course (cname, user_id, cstatus)
        values (#{cname}, #{userId}, #{cstatus})
    </insert>
</mapper>